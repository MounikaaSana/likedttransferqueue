import java.util.ArrayList;
import java.util.List;
import java.util.concurrent.PriorityBlockingQueue;
import java.util.Comparator;
public class Main{
	public static void main(String[] args) {
	    int capacity=10;
	    PriorityBlockingQueue pbq=new PriorityBlockingQueue(capacity,Comparator.reverseOrder());
	    pbq.add(0);
	    pbq.add(1);
	    pbq.add(2);
	    pbq.add(3);
	    pbq.add(4);
	    pbq.add(5);
	    pbq.add(6);
	    pbq.add(7);
	    pbq.add(8);
	    pbq.add(9);
	    pbq.add(10);
	    pbq.add(11);
	    System.out.println(pbq);
	}
}
